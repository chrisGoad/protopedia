{
  "rules": {
    ".read": true,
    "directory":{
       "$uid":{
           ".write": "auth.uid === $uid"
        }
    },
    "account":{
       "$uid":{
           ".write": "auth.uid === $uid"
        },
        ".indexOn":["userName"]
    },
    "users":{
      "$any":{
         ".write":"!data.exists()"
      }
    }
  }
}
