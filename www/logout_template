{{boiler0}}

<script src="http://{{domain}}/js/pjloginout-{{pjloginout_version}}.js"></script>

<script>

(function (__pj__) {
  $('document').ready(function () {
    var om = __pj__.om;
    var ui = __pj__.ui;
    var user = __pj__.user;
    // check if logged in with persona
    var prs = user.signedInWithPersona();
    if (prs) {
      __pj__.user.personaSetup();
      navigator.id.logout();
    } 
    om.ajaxPost('/api/logOut',{},
      function (rs) {
        om.clearStorageOnLogout();
        var dst = ui.isDev?'http://prototype-jungle.org:8000':'http://prototypejungle.org';
        dst += "#logout=1";
        setTimeout(function () {location.href = dst;},1000);         
 
      });
   });
})(prototypeJungle)
</script>

{{boiler2}}

  <div id="mainDiv">
    <div style="width:100%;text-align:center"><b>You are now logged out.</b></div>

  </div>
  <div id="results"></div>
</div>

