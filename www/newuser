$('document').ready(function () {
    var ui = pj.ui;
    pj.om.checkSession(function (rs) {
      ui.signInOutHandler();
      ui.genTopbar($('#topbar'),{includeTitle:1});
    });
    var hnd = localStorage.handle;
    $('#mainDiv').html('You have been allocated http://prototypejungle/'+hnd+', where you can save  your own variants of any item, '+
                           'and build new items from JavaScript source. The File pulldown is your entryway.');
 
});

</script>

